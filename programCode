# Program which pulls expiration data value based on Agent key
while True:
    print("Hello! Welcome to LicenseFinder version 1.0!")
    print("Please enter the agent name you want to search:")
    dateInput = input()
    if dateInput == '':
        break

    if dateInput in myAgents:
        print(myAgents[dateInput] + ' is the VA license expiration date of ' + dateInput + ".")
        print("")
        print("Do you want to search again?")
        againAnswer = input()
        if againAnswer == "Yes":
            continue
        elif againAnswer == "No":
            break
    else:
        print('I am sorry, I do not find your search in the dictionary.')
